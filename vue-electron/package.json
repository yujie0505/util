    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "electron": "^13.6.6",
    "electron-packager": "^13.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.1",
    "url-loader": "^1.1.2",
